{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-create-product": "cd nova-components/CreateProduct && npm run dev",
        "build-create-product-prod": "cd nova-components/CreateProduct && npm run prod",
        "build-product-attributes": "cd nova-components/ProductAttributes && npm run dev",
        "build-product-attributes-prod": "cd nova-components/ProductAttributes && npm run prod",
        "build-attribute-group-attributes": "cd nova-components/AttributeGroupAttributes && npm run dev",
        "build-attribute-group-attributes-prod": "cd nova-components/AttributeGroupAttributes && npm run prod",
        "build-create-procurement-plan": "cd nova-components/CreateProcurementPlan && npm run dev",
        "build-create-procurement-plan-prod": "cd nova-components/CreateProcurementPlan && npm run prod",
        "build-create-procurement-plan-tool": "cd nova-components/CreateProcurementPlanTool && npm run dev",
        "build-create-procurement-plan-tool-prod": "cd nova-components/CreateProcurementPlanTool && npm run prod",
        "build-create-resource-button": "cd nova-components/CreateResourceButton && npm run dev",
        "build-create-resource-button-prod": "cd nova-components/CreateResourceButton && npm run prod",
        "build-amazon-mws": "cd nova-components/AmazonMws && npm run dev",
        "build-amazon-mws-prod": "cd nova-components/AmazonMws && npm run prod",
        "build-storage-select-product": "cd nova-components/StorageSelectProduct && npm run dev",
        "build-storage-select-product-prod": "cd nova-components/StorageSelectProduct && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.0.0",
        "popper.js": "^1.12",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "nova-watcher": "^1.0.1"
    }
}