{
    "name": "chang/variant-provider",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Chang\\VariantProvider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chang\\VariantProvider\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
